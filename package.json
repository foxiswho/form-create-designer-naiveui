{
    "name": "form-designer-naiveui",
    "version": "1.0.10",
    "description": "好用的vue可视化表单设计器组件",
    "unpkg": "./dist/index.umd.js",
    "jsdelivr": "./dist/index.umd.js",
    "typings": "./types/index.d.ts",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./locale/en": {
            "import": "./locale/en.mjs",
            "require": "./locale/en.umd.js"
        },
        "./locale/zh-cn": {
            "import": "./locale/zh-cn.mjs",
            "require": "./locale/zh-cn.umd.js"
        }
    },
    "scripts": {
        "clean": "rimraf dist/",
        "dev": "vite",
        "rollup": "rollup -c ./rollup.config.ts",
        "build": "vite build --config ./vite.config.build.js",
        "build:locale": "gulp -f gulpfile.js",
        "build:preview": "vite build --config ./vite.config.preview.js",
        "build:all": "vite build",
        "preview": "vite preview"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/foxiswho/form-create-designer-naiveui.git"
    },
    "keywords": [
        "表单设计器",
        "@form-create",
        "form-builder",
        "form-designer",
        "draggable",
        "form",
        "components",
        "vue3",
        "naive-ui",
        "json-form",
        "dynamic-form"
    ],
    "files": [
        "README.md",
        "package.json",
        "LICENSE",
        "CHANGELOG.md",
        "src",
        "types",
        "dist",
        "locale"
    ],
    "author": "foxiswho",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/foxiswho/form-create-designer-naiveui/issues"
    },
    "homepage": "http://designer.form-create.com",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@element-plus/icons-vue": "^2.3.1",
        "@sixian/css-url": "^1.0.3",
        "@types/shelljs": "^0.8.15",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/babel-plugin-jsx": "^1.2.2",
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-service": "^5.0.8",
        "@vue/compiler-sfc": "^3.4.21",
        "babel-eslint": "^10.1.0",
        "chalk": "^5.3.0",
        "commander": "^12.0.0",
        "commitizen": "^4.3.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.10.0",
        "cssnano": "^6.1.0",
        "cssnano-preset-advanced": "^6.1.0",
        "cz-conventional-changelog": "^3.3.0",
        "cz-customizable": "^7.0.0",
        "dayjs": "^1.11.10",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.23.0",
        "esno": "^4.7.0",
        "execa": "^8.0.1",
        "fast-glob": "^3.3.2",
        "figlet": "^1.7.0",
        "fs-extra": "^11.2.0",
        "gulp": "^4.0.2",
        "html-webpack-plugin": "^5.6.0",
        "humps": "^2.0.1",
        "husky": "^9.0.11",
        "jsonlint-mod": "^1.7.6",
        "lint-staged": "^15.2.2",
        "naive-ui": "^2.38.1",
        "npm-run-all": "^4.1.5",
        "ora": "^8.0.1",
        "postcss": "^8.4.37",
        "rimraf": "^5.0.5",
        "rollup-plugin-visualizer": "^5.12.0",
        "shelljs": "^0.8.5",
        "stringify-author": "^0.1.3",
        "tslib": "^2.6.2",
        "typescript": "^5.4.2",
        "vite": "^5.2.0",
        "vite-plugin-banner": "^0.7.1",
        "vite-plugin-css-injected-by-js": "^3.5.0",
        "vue": "^3.4.21",
        "vue-loader": "^17.4.2",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.7.16"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,vue}": [
            "eslint --fix",
            "git add"
        ]
    },
    "dependencies": {
        "@codemirror/autocomplete": "^6.15.0",
        "@codemirror/lang-javascript": "^6.2.2",
        "@codemirror/lang-json": "^6.0.1",
        "@codemirror/theme-one-dark": "^6.1.2",
        "@form-create/component-wangeditor": "^3.1.20",
        "@form-create/naive-ui": "^3.1.28",
        "@form-create/utils": "^3.1.23",
        "codemirror": "^6.0.1",
        "vite-plugin-html": "^3.2.2",
        "vuedraggable": "^4.1.0"
    }
}
